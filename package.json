{
  "name": "qaq-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "commit:comment": "引导设置规范化的提交信息",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "types": [
        { "emoji": "✨", "code": "feat:", "description": "引入新功能", "name": "feature" },
        { "emoji": "🐛", "code": "fix:", "description": "修复bug", "name": "fix" },
        { "emoji": "📝", "code": "docs:", "description": "修改文档", "name": "docs" },
        { "emoji": "🎨", "code": "style:", "description": "优化代码结构或格式", "name": "style" },
        { "emoji": "🚀", "code": "perf:", "description": "优化性能", "name": "perf" },
        { "emoji": "🔨", "code": "refactor:", "description": "重构代码", "name": "refactor" },
        { "emoji": "💄", "code": "ui:", "description": "更新UI样式", "name": "ui" },
        { "emoji": "🔧", "code": "config:", "description": "修改配置文件", "name": "config" },
        { "emoji": "🚨", "code": "test:", "description": "增加测试", "name": "test" },
        { "emoji": "🚧", "code": "wip:", "description": "进行中的工作", "name": "wip" }
      ],
      "allowCustomScopes": true,
      "allowBreakingChanges": ["feat", "fix"]
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.7",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/node": "^20.4.1",
    "commitizen": "^4.3.0",
    "cz-emoji": "1.3.2-canary.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lerna": "^7.1.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
